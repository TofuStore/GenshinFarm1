<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thisotheraccount.genshinfarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/paimon_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/paimon_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GenshinFarm">
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.DiligenceReceiver">
        </receiver>
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.ProsperityReceiver">
        </receiver>
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.GoldReceiver">
        </receiver>
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.FreedomReceiver">
        </receiver>
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.ResistanceReceiver">
        </receiver>
        <receiver
            android:name="com.example.thisotheraccount.genshinfarm.BalladReceiver">
        </receiver>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>